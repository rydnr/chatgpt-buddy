{
  "name": "@google-buddy/client",
  "version": "1.0.0",
  "description": "Google automation wrapper for Web-Buddy framework",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "clean": "rimraf dist"
  },
  "keywords": [
    "google",
    "search-automation",
    "web-buddy",
    "browser-automation"
  ],
  "author": "rydnr@acm-sl.org",
  "license": "GPL-3.0",
  "dependencies": {
    "@web-buddy/core": "workspace:*"
  },
  "devDependencies": {
    "@playwright/test": "^1.39.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@web-buddy/core": "^1.0.0",
    "typescript": "^5.0.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ]
