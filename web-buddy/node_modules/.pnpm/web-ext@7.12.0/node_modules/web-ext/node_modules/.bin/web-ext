#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/chous/github/rydnr/chatgpt-buddy/web-buddy/node_modules/.pnpm/web-ext@7.12.0/node_modules/web-ext/bin/node_modules:/home/chous/github/rydnr/chatgpt-buddy/web-buddy/node_modules/.pnpm/web-ext@7.12.0/node_modules/web-ext/node_modules:/home/chous/github/rydnr/chatgpt-buddy/web-buddy/node_modules/.pnpm/web-ext@7.12.0/node_modules:/home/chous/github/rydnr/chatgpt-buddy/web-buddy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/chous/github/rydnr/chatgpt-buddy/web-buddy/node_modules/.pnpm/web-ext@7.12.0/node_modules/web-ext/bin/node_modules:/home/chous/github/rydnr/chatgpt-buddy/web-buddy/node_modules/.pnpm/web-ext@7.12.0/node_modules/web-ext/node_modules:/home/chous/github/rydnr/chatgpt-buddy/web-buddy/node_modules/.pnpm/web-ext@7.12.0/node_modules:/home/chous/github/rydnr/chatgpt-buddy/web-buddy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/web-ext.js" "$@"
else
  exec node  "$basedir/../../bin/web-ext.js" "$@"
fi
