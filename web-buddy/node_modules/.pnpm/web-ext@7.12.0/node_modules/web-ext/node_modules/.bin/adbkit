#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/chous/github/rydnr/chatgpt-buddy/web-buddy/node_modules/.pnpm/@devicefarmer+adbkit@3.2.3/node_modules/@devicefarmer/adbkit/bin/node_modules:/home/chous/github/rydnr/chatgpt-buddy/web-buddy/node_modules/.pnpm/@devicefarmer+adbkit@3.2.3/node_modules/@devicefarmer/adbkit/node_modules:/home/chous/github/rydnr/chatgpt-buddy/web-buddy/node_modules/.pnpm/@devicefarmer+adbkit@3.2.3/node_modules/@devicefarmer/node_modules:/home/chous/github/rydnr/chatgpt-buddy/web-buddy/node_modules/.pnpm/@devicefarmer+adbkit@3.2.3/node_modules:/home/chous/github/rydnr/chatgpt-buddy/web-buddy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/chous/github/rydnr/chatgpt-buddy/web-buddy/node_modules/.pnpm/@devicefarmer+adbkit@3.2.3/node_modules/@devicefarmer/adbkit/bin/node_modules:/home/chous/github/rydnr/chatgpt-buddy/web-buddy/node_modules/.pnpm/@devicefarmer+adbkit@3.2.3/node_modules/@devicefarmer/adbkit/node_modules:/home/chous/github/rydnr/chatgpt-buddy/web-buddy/node_modules/.pnpm/@devicefarmer+adbkit@3.2.3/node_modules/@devicefarmer/node_modules:/home/chous/github/rydnr/chatgpt-buddy/web-buddy/node_modules/.pnpm/@devicefarmer+adbkit@3.2.3/node_modules:/home/chous/github/rydnr/chatgpt-buddy/web-buddy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@devicefarmer+adbkit@3.2.3/node_modules/@devicefarmer/adbkit/bin/adbkit" "$@"
else
  exec node  "$basedir/../../../../../@devicefarmer+adbkit@3.2.3/node_modules/@devicefarmer/adbkit/bin/adbkit" "$@"
fi
